{
  "name": "lowdb",
  "version": "7.0.0",
  "description": "Tiny local JSON database for Node & Electron",
  "keywords": [
    "database",
    "db",
    "electron",
    "embed",
    "embedded",
    "flat",
    "JSON",
    "local",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiller619/lowdb.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "yarn tsup",
    "lint": "eslint --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "prepare": "yarn husky install && yarn build"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "dts": true,
    "clean": true,
    "format": "esm"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "steno": "^3.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.6.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tempy": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.19"
}
